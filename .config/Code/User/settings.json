{
    "git.ignoreLegacyWarning": true,
    "files.autoSave": "off",
    "editor.fontFamily": "Hack",
    "editor.tabSize": 4,
    "editor.cursorStyle": "block",
    "editor.insertSpaces": true,
    "files.eol": "\n",
    "workbench.iconTheme": "vscode-icons",
    "files.exclude": {
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/CVS": true,
        "**/.DS_Store": true
    },
    "window.menuBarVisibility": "toggle",
    "window.zoomLevel": 0,
    "vscodeWorkspaceSwitcher.paths": [
        "~/vs-code"
    ],
    "vim.neovimPath": "/home/developer/neovim/bin/nvim",
    "vim.enableNeovim": false,
    "workbench.startupEditor": "welcomePage",
    "workbench.colorTheme": "Plastic",
    "vim.overrideCopy": true,
    "vim.useSystemClipboard": true,
    "vim.useCtrlKeys": true,
    "vim.handleKeys": {
        "<C-c>": false,
        "<C-v>": false,
        "<C-a>": false,
        "<C-f>": false,
        "<C-t>": false,
        "<C-z>": false,
        "<C-y>": false
    },
    "vim.cmdLineInitialColon": true,
    "vim.visualstar": true,
    "vim.cursorStylePerMode": {
        "normal": "underline",
        "insert": "line-thin",
        "replace": "block-outline"
    },
    "vim.smartcase": true,
    "vim.easymotion": true,
    "vim.sneak": true,
    "vim.hlsearch": true,
    "gitlens.advanced.messages": {
        "suppressCommitHasNoPreviousCommitWarning": false,
        "suppressCommitNotFoundWarning": false,
        "suppressFileNotUnderSourceControlWarning": false,
        "suppressGitVersionWarning": true,
        "suppressLineUncommittedWarning": false,
        "suppressNoRepositoryWarning": false,
        "suppressResultsExplorerNotice": false,
        "suppressShowKeyBindingsNotice": true
    },
    "editor.matchBrackets": false,
    "clang-format.style": "file",
    "clang-format.assumeFilename": "~/.clang-format",
    "C_Cpp.dimInactiveRegions": false,
    "editor.dragAndDrop": false,
    "window.openFilesInNewWindow": "on",
    "window.restoreWindows": "all",
    "editor.smoothScrolling": true,
    "workbench.editor.enablePreview": false,
    "workbench.editor.enablePreviewFromQuickOpen": false,
    "workbench.editor.revealIfOpen": true,
    "workbench.statusBar.feedback.visible": false,
    "workbench.editor.showTabs": true,
    "terminal.explorerKind": "internal",
    "terminal.external.linuxExec": "terminology",
    "editor.codeLens": true,
    "symbolOutline.doSort": true,
}